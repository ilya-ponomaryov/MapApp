<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.mapapp.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map">
        <action
            android:id="@+id/action_mapFragment_to_markersFragment"
            app:destination="@id/markersFragment" />
    </fragment>
    <fragment
        android:id="@+id/markersFragment"
        android:name="com.example.mapapp.markers.MarkersFragment"
        android:label="fragment_markers"
        tools:layout="@layout/fragment_markers" />

</navigation>